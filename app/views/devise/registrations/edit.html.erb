

<div class="panel panel-default">
  <div class="panel-heading">
    <div class="panel-title">
      <h1>Edit <%= resource_name.to_s.humanize %></h1>
    </div>
  </div>
  <div class="panel-body">
    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :method => :put }) do |f| %>
      <%= devise_error_messages! %>

      <div class="form-group">
        <%= f.label :name_user %>
        <%= f.text_field :name_user, class: "form-control", :autofocus => true %>
      </div>

      <div class="form-group">
        <%= f.label :email %>
        <%= f.email_field :email, class: "form-control", :autofocus => false %>
      </div>

      <div class="form-group">
        <%= f.label :password %> <i>(leave blank if you don't want to change it)</i>
        <%= f.password_field :password, class: "form-control", :autocomplete => "off" %>
      </div>

      <div class="form-group">
        <%= f.label :current_password %> <i>(we need your current password to confirm your changes)</i>
        <%= f.password_field :current_password, class: "form-control" %>
      </div>


      <span class="Datos-Usuario"><h3> Datos del Usuario</h3></span>

      <div class="form-group">
        <%= f.label :name_country %>
        <%= f.text_field :name_country, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :estado_id %>
        <%= f.collection_select(:estado_id, Estado.all, :id, :descripcion, :include_blank => "Please select an Status ...") %>
      </div>

      <div class="form-group">
        <%= f.check_box :notificar_usuario %>
        <%= f.label :notificar_usuario %>
      </div>

      <span class="Datos-Usuario"><h3> Datos de la Empresa </h3></span>

      <div class="form-group">
        <%= f.label :nombre_empresa %>
        <%= f.text_field :nombre_empresa, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :logo %>
        <%= f.text_field :logo, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :direccion %>
        <%= f.text_field :direccion, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :codigo_postal %>
        <%= f.text_field :codigo_postal, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :name_country_procedence %>
        <%= f.text_field :name_country_procedence, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :demanda_productos_servicios %>
        <%= f.text_field :demanda_productos_servicios, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :oferta_productos_servicios %>
        <%= f.text_field :oferta_productos_servicios, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :rango_empleados %>
       
<%= select("user", "rango_empleados", {"1-10" => "1", "11-20" => "2", "21-50" => "3", "51-100" => "4", "101-1000" => "5", ">1001" =>"6"}) %>
      </div>

      <div class="form-group">
        <%= f.label :rango_ventas %>
        <%= f.text_field :rango_ventas, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :url %>
        <%= f.text_field :url, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :facebook %>
        <%= f.text_field :facebook, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :twitter %>
        <%= f.text_field :twitter, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :google_plus %>
        <%= f.text_field :google_plus, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :linkedin %>
        <%= f.text_field :linkedin, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :pinteresting %>
        <%= f.text_field :pinteresting, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.submit "Update", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
  <div class="panel-footer">
    <h3>Cancel my account</h3>

    <p>Unhappy? <%= button_to "Cancel my account", registration_path(resource_name), :data => { :confirm => "Are you sure?" }, :method => :delete, class: "btn btn-warning" %></p>

    <%= link_to "Back", :back %>
  </div>
</div>


